    "ts-loader": "^9.5.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
